{
	"name": "karma-tests",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --configuration=production",
		"watch": "ng build --watch --configuration development",
		"prod": "ng serve --prod",
		"test": "ng test --watch=false",
		"generate": "node ./database/generateDB.js > ./database/database.json",
		"server": "json-server --watch ./database/database.json",
		"ci": "npm run lint && npm run test && npm run build",
		"lint": "eslint . --ext .ts",
		"pretty": "npx prettier --write ."
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^14.0.0-next.0",
		"@angular/common": "^14.0.0-next.0",
		"@angular/compiler": "^14.0.0-next.0",
		"@angular/core": "^14.0.0-next.0",
		"@angular/forms": "^14.0.0-next.0",
		"@angular/platform-browser": "^14.0.0-next.0",
		"@angular/platform-browser-dynamic": "^14.0.0-next.0",
		"@angular/router": "^14.0.0-next.0",
		"rxjs": "~7.5.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^14.2.0",
		"@angular/cli": "~14.2.0",
		"@angular/compiler-cli": "^14.0.0-next.0",
		"@types/jasmine": "~4.0.0",
		"@types/karma-chrome-launcher": "^3.1.1",
		"@types/karma-coverage": "^2.0.1",
		"@types/karma-jasmine": "^4.0.2",
		"@types/karma-jasmine-html-reporter": "^1.7.0",
		"@typescript-eslint/eslint-plugin": "^5.35.1",
		"@typescript-eslint/parser": "^5.35.1",
		"eslint": "^8.23.0",
		"jasmine": "^4.3.0",
		"jasmine-core": "~4.3.0",
		"jasmine-spec-reporter": "^7.0.0",
		"jasmine-ts": "^0.4.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.0.0",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.9.1",
		"typescript": "~4.7.2",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.10.0"
	}
}
